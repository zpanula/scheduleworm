{
  "name": "scheduleworm",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "lint": "eslint . --ext .js",
    "test": "run-s -c lint jest coverage:badge",
    "coverage:badge": "coverage-badge-creator",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node src/app.js"
  },
  "jest": {
    "transform": {},
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coveragePathIgnorePatterns": [
      "bootstrap.min.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    },
    "coverageReporters": [
      "json-summary",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cygroupa/scheduleworm.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/cygroupa/scheduleworm/issues"
  },
  "homepage": "https://github.com/cygroupa/scheduleworm#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "ejs": "^3.0.2",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.10",
    "pino": "^7.10.0"
  },
  "devDependencies": {
    "coverage-badge-creator": "^1.0.11",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-env-example": "^0.0.0",
    "gen-env-template": "^2.0.6",
    "jest": "^27.5.1",
    "jest-coverage-badges": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2"
  }
}
