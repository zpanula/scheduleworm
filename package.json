{
  "name": "scheduleworm",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "npm run lint && jest --passWithNoTests",
    "clean": "rm -rf ./dist",
    "copy-files": "cp -r ./src/views ./dist && cp -r ./src/public/css ./dist/public/css && cp ./src/public/js/{*.js,*.map} ./dist/public/js",
    "build": "npm run clean && tsc && npm run copy-files",
    "start": "node dist/app.js",
    "dev": "node --loader ts-node/esm src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cygroupa/scheduleworm.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/cygroupa/scheduleworm/issues"
  },
  "homepage": "https://github.com/cygroupa/scheduleworm#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "ejs": "^3.0.2",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.10",
    "nanoid": "^3.3.3",
    "pino": "^7.10.0",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.5",
    "swagger-jsdoc": "^6.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.32",
    "@types/swagger-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-env-example": "^0.0.0",
    "gen-env-template": "^2.0.6",
    "jest": "^27.5.1",
    "onchange": "^7.1.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
